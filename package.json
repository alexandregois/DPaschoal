{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start-kdp",
    "start-kdp": "ng serve kdp --port 4200",
    "start-dpk": "ng serve dpk --port 4201",
    "watch": "ng build --watch --configuration development",
    "test": "ng test dpk",
    "lint": "ng lint dpk --fix",
    "lint-husky": "ng lint dpk",
    "test-husky": "ng test dpk --watch=false --code-coverage --progress",
    "prettier": "prettier --write .",
    "prettier-husky": "prettier --write",
    "prepare": "husky install",
    "generate-api": "npm run generate-api-dev",
    "generate-api-dev": "node --trace-uncaught openapitools.setup.js env=dev && openapi-generator-cli generate --generator-key dev",
    "generate-api-qas": "node --trace-uncaught openapitools.setup.js env=qas && openapi-generator-cli generate --generator-key qas",
    "generate-api-prd": "node --trace-uncaught openapitools.setup.js env=prd && openapi-generator-cli generate --generator-key prd",
    "build-dev-kdp": "ng build kdp --configuration development",
    "build-dev-dpk": "ng build dpk --configuration development",
    "build-qas-kdp": "ng build kdp --configuration homolog",
    "build-qas-dpk": "ng build dpk --configuration homolog",
    "build-prd-kdp": "ng build kdp --configuration production",
    "build-prd-dpk": "ng build dpk --configuration production",
    "build": "npm run build-dev",
    "build-dev": "npm run generate-api-dev && npm run build-dev-kdp && npm run build-dev-dpk",
    "build-qas": "npm run generate-api-qas && npm run build-qas-kdp && npm run build-qas-dpk",
    "build-prd": "npm run generate-api-prd && npm run build-prd-kdp && npm run build-prd-dpk"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.7",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.2.7",
    "@angular/compiler": "14.2.7",
    "@angular/core": "14.2.7",
    "@angular/forms": "14.2.7",
    "@angular/material": "14.2.7",
    "@angular/platform-browser": "14.2.7",
    "@angular/platform-browser-dynamic": "14.2.7",
    "@angular/router": "14.2.7",
    "@lyracom/embedded-form-glue": "^1.3.1",
    "@nestjs/common": "^10.2.7",
    "@ngneat/input-mask": "^6.1.0",
    "credit-card-type": "^9.1.0",
    "inputmask": "^5.0.8",
    "ngx-mask": "^14.2.4",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.7",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "14.2.7",
    "@angular/compiler-cli": "14.2.7",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/inputmask": "^5.0.3",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-download": "^0.1.2",
    "husky": "^8.0.1",
    "jasmine-core": "^4.3.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "typescript": "4.8.4"
  }
}
